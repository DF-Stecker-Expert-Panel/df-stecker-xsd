<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="DR-GW" targetNamespace="DR-GW" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <!-- *************************************************************** -->
  <xs:include schemaLocation="DR-GW-Group.get.xsd"/>
  <xs:include schemaLocation="DR-GW-Group.getList.xsd"/>
  <xs:include schemaLocation="DR-GW-Group.getRadioMembers.xsd"/>
