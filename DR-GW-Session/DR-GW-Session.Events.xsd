<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="DR-GW-Interface/DR-GW-Session.Events"
  targetNamespace="DR-GW-Interface/DR-GW-Session.Events" elementFormDefault="qualified"
